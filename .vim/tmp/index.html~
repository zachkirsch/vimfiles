<script type="text/javascript" src="js/jquery-2.1.1.min.js">
</script>
<script type="text/javascript" src="js/bootstrap.min.js">
</script>
<script type="text/javascript">


    $(".nav-lg").hide()
    $(".nav-sm").hide()
    $(document).ready(function() {


      var myWindow = $(window)
      $(".camp").click(function() {
        window.location = 'http://vbidebate.com';
      });
      $(".blog").click(function() {
        window.location = 'http://vbriefly.com';
      });
      $(".briefs").click(function() {
        window.location = 'store';
      });
      $(".logo").click(function() {
        window.location = "#";
      });
      updateBillboardHeight()
      // Resize event handler
      if (myWindow.width() > 992) {
        $(".nav-lg").show()
        $(".nav-sm").hide()
      }
      else {
        $(".nav-lg").hide()
        $(".nav-sm").show()
      }
      myWindow.resize(function() {
        if (myWindow.width() > 992) {
          $(".nav-lg").show()
          $(".nav-sm").hide()
        }
        else {
          $(".nav-lg").hide()
          $(".nav-sm").show()
        }
        updateBillboardHeight()
      });

      function updateBillboardHeight() {
        $("header").height(myWindow.height() - ($("#about").height() - Math.abs(parseInt($("#about").css("marginTop")))))
      }

      $(".contact").click(function() {
        $("#contact-modal").modal('show');
      });
      $("#contact-modal .cancel").click(function() {
        /* Clear the inputs */
        $('#username, #useremail, #message').val('');
      });

      $(".submit").click(function() {
        $.post('php/contact.php', {
            username: $('#username').val(),
            useremail: $('#useremail').val(),
            subject: $('#subject').val(),
            message: $('#message').val(),
            myFormSubmitted: 'yes'
          }, function(data) {
            $("#formResponse").html(data).fadeIn('100');
          }, 'text'
        );
        $("#form-success").fadeIn('slow');

        /* Clear the inputs */
        $('#username, #useremail, #message').val('');
      });

    });
</script>
