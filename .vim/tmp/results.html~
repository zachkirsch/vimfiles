    <script src="logic.js"></script>
    <script src="get_orders.js"></script>
    <script>
        $(document).ready(function () {
            query = getQueryParams(document.location.search);
            
            // callback function to place items in the necessary location
            get_orders(query.fooditem, function (orders) {

				orders.sort(function(a,b) {
					a_fee = parseFloat(a.restaurant.min) + parseFloat(a.restaurant.fee);
					b_fee = parseFloat(b.restaurant.min) + parseFloat(b.restaurant.fee);
					diff = a_fee - b_fee;
  					if (diff < 0)
     					return -1;
  					if (diff > 0)
    					return 1;
					return 0;
				});

                $.get('/salesTax' + '?zip=' + query.zip, function(tax_rate) {
                    if (orders.length == 0) {
                        $("#results-title")[0].innerText = "No Results";
                        $("#results-title").css("height", "430px");
                    }
                    for (j = 0; j < orders.length; j++) {
                        order = orders[j];
            
                        // delivery minimum
                        $("#rest" + j)[0].children[1].children[1].children[1].firstElementChild.children[0].children[1].innerText = '$' + parseFloat(order.restaurant.min).toFixed(2);
                        // delivery fee
                        $("#rest" + j)[0].children[1].children[1].children[1].firstElementChild.children[1].children[1].innerText = '$' + parseFloat(order.restaurant.fee).toFixed(2);


                        $("#rest" + j)[0].children[0].children[0].children[0].innerText = order.restaurant.name;

                        sum = 0;
                        for (i = 0; i < order.items.length; i++) {
                            if (order.items[i] == undefined) continue;
                            if (parseFloat(order.items[i].price) == 0) continue;
                            $("<tr id='row" + i + "'><td id='order" + i + "'>" + order.items[i].food + "</td><td class='spacer'></td><td></td><td id='price" + i + "'> $" + parseFloat(order.items[i].price).toFixed(2) + "</td><td id='tip-col'></td></tr>").insertBefore("#row-sub-" + j);
                            sum += order.items[i].price;
                        }
                        $("#row-sub-" + j)[0].children[3].innerText = '$' + (sum).toFixed(2);
                        $("#tax-"+j)[0].innerText = '$' + (tax_rate  * sum).toFixed(2);
                        $("#rest" + j )[0].children[1].children[1].children[0].firstElementChild.firstElementChild.firstElementChild.firstElementChild.firstElementChild.innerText = (parseFloat(sum) * (1+parseFloat(tax_rate)+ 0.15) + parseFloat(order.restaurant.fee)).toFixed(2);

                    }
                });
                for (k=9; k > (orders.length-1); k--) $("#rest" + k).remove();
                setTimeout(function() {
                    $(".loader").fadeOut("slow");
                },1000);
            });
            
            $(".tip_click").click(function () {                
                var percent = $(this)[0].attributes.value.value; 
                var subtotal = $("#before_tip")[0].innerText;
                var tip = (percent * subtotal).toFixed(2);
                var tax = $("#chosenTax")[0].innerText;
                var fee = $("#delivery_fee")[0].innerText;
                var total = (parseFloat(subtotal) + parseFloat(tip) + parseFloat(tax) + parseFloat(fee)).toFixed(2);
                $("#tip_amount")[0].value = tip;
                $("#total_price")[0].innerText = total;

            });                


            $(".btn-md").click(function () {
                $("#jumbo-sum").removeClass("hidden");
                var rest_text = $(this)[0].parentElement.firstElementChild.innerText; 
                var num_items = -3 + $(this)[0].parentElement.parentElement.parentElement.children[1].children[0].children[0].children[0].children[0].children[0].children.length;
                var items = '';
                $("#chosenRest")[0].innerText = rest_text;
                for (i = 0; i < num_items; i++) {
                    var item = $(this).parent().parent().parent().children()[1].children[0].children[0].children[0].children[0].children[0].children[i + 1].children[0].innerText;
                    if (i != 0) {
                        items = items + ', ';
                    }
                    items = items + item;
                }
                
                $("#restItems")[0].innerText = items;   

                var subtotal = parseFloat($(this).parent().parent().parent().children()[1].children[0].children[0].children[0].children[0].children[0].children[num_items + 1].children[3].innerText.replace(/\$|,/g, '')); 
                $("#before_tip")[0].innerText = subtotal;
                var tax = parseFloat($(this).parent().parent().parent().children()[1].children[0].children[0].children[0].children[0].children[0].children[num_items + 2].children[3].innerText.replace(/\$|,/g, '')); 
                var fee = $(this).parent().parent().parent().children()[1].children[1].children[1].children[0].children[1].children[1].innerText.replace(/\$|,/g, '');
                $("#delivery_fee")[0].innerText = fee;
                $("#chosenTax")[0].innerText = tax;
                $("#total_price")[0].innerText = (subtotal * 1.15 + tax + parseFloat(fee)).toFixed(2);  
                $("#tip_amount")[0].value = (0.15 * subtotal).toFixed(2);
            });

            $("#tip_amount").change(function () {
                var subtotal = $("#before_tip")[0].innerText.replace(/\$|,/g, '');
                var tax = $("#chosenTax")[0].innerText;
                var tip = $("#tip_amount")[0].value;
                var fee = $("#delivery_fee")[0].innerText;
                if (tip == '') {
                    $("#total_price")[0].innerText = (parseFloat(subtotal) + parseFloat(tax) + parseFloat(fee));
                } else {
                    $("#total_price")[0].innerText = (parseFloat(subtotal) + parseFloat(tax) + parseFloat(tip) +parseFloat(fee)).toFixed(2);
                }
            });

            $("#email-button").click(function() {
                $.get("/sendEmail" + '?name=' + $("#username")[0].value + "&restaurant=" + $("#chosenRest")[0].innerText + "&price=" + $("#total_price")[0].innerText.replace(/\$|,/g, '') + "&email=" + $("#email")[0].value + "&order=" + $("#restItems")[0].innerText);
            });

        });

        // gracias http://stackoverflow.com/questions/979975/how-to-get-the-value-from-the-url-parameter
        function getQueryParams(qs) {
            qs = qs.split("+").join(" ");

            var params = {},
                tokens,
                re = /[?&]?([^=]+)=([^&]*)/g;

            while (tokens = re.exec(qs)) {
                params[decodeURIComponent(tokens[1])] = decodeURIComponent(tokens[2]);
            }

            return params;
        }
    </script>
