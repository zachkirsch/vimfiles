#!/bin/bash

#61 kegs, 41,521 files, 781.6M

failed_items=""
function install_package() {
echo EXECUTING: brew install $1 $2
brew install $1 $2
[ $? -ne 0 ] && $failed_items="$failed_items $1" # package failed to install.
}
brew tap homebrew/core
install_package ant ''
